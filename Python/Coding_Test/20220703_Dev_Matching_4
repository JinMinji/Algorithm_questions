
(SELECT '01' MON, PLACE_ID, COUNT(PLACE_ID) CNT
    FROM PLACE_REVIEWS
    WHERE DATE_FORMAT(CREATED_AT,'%Y-%m') ='2018-01'
    ORDER BY CNT
    LIMIT 1
)
UNION ALL
(SELECT '02' MON, PLACE_ID, COUNT(PLACE_ID) CNT
    FROM PLACE_REVIEWS
    WHERE DATE_FORMAT(CREATED_AT,'%Y-%m') ='2018-02'
    ORDER BY CNT
    LIMIT 1
)
UNION ALL
(SELECT '03' MON, PLACE_ID, COUNT(PLACE_ID) CNT
    FROM PLACE_REVIEWS
    WHERE DATE_FORMAT(CREATED_AT,'%Y-%m') ='2018-03'
    ORDER BY CNT
    LIMIT 1
)
UNION ALL
(SELECT '04' MON, PLACE_ID, COUNT(PLACE_ID) CNT
    FROM PLACE_REVIEWS
    WHERE DATE_FORMAT(CREATED_AT,'%Y-%m') ='2018-04'
    ORDER BY CNT
    LIMIT 1
)
UNION ALL
