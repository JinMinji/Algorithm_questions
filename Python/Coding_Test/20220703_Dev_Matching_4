    FROM PLACE_REVIEWS
    WHERE DATE_FORMAT(CREATED_AT,'%Y-%m') ='2018-05'
    ORDER BY CNT
    LIMIT 1
)
UNION ALL
(SELECT '06' MON, PLACE_ID, COUNT(PLACE_ID) CNT
    FROM PLACE_REVIEWS
    WHERE DATE_FORMAT(CREATED_AT,'%Y-%m') ='2018-06'
    ORDER BY CNT
    LIMIT 1
)
